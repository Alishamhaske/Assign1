
@{
    ViewData["Title"] = "ProductList";
}

<h1>Assignment 3</h1>
<h3>ProductList</h3>

<table class="table table-danger">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Price</th>
        </tr>
    </thead>

    <tbody>
        @foreach(Product p in ViewBag.List)
        {
            <tr>
                <td>@p.Id</td>
                <td>@p.Name</td>
                <td>@p.Price</td>
            </tr>
        }
    </tbody>

</table>

